{
  "workbench.startupEditor": "newUntitledFile",
  "editor.cursorStyle": "block",
  "editor.tabSize": 2,
  "breadcrumbs.enabled": true,
  "terminal.integrated.copyOnSelection": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "html.format.endWithNewline": true,
  "editor.renderWhitespace": "boundary",
  "editor.wordWrap": "wordWrapColumn",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.fontFamily": "Fira Code",
  "window.newWindowDimensions": "inherit",
  "editor.rulers": [100],
  "javascript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "editor.formatOnSave": true,
  "workbench.fontAliasing": "antialiased",
  "editor.fontLigatures": true,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": 0,
  "go.autocompleteUnimportedPackages": true,
  "emmet.showSuggestionsAsSnippets": true,
  "typescript.surveys.enabled": false,
  "typescript.suggestionActions.enabled": false,
  "typescript.tsc.autoDetect": "off",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "workbench.tree.indent": 24,
  "terminal.integrated.rendererType": "experimentalWebgl",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "githubPullRequests.telemetry.enabled": false,
  "editor.semanticHighlighting.enabled": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.minimap.showSlider": "always",
  "terminal.external.osxExec": "iTerm.app",
  "workbench.sideBar.location": "right",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "go.useLanguageServer": true,
  "diffEditor.codeLens": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.tabCloseButton": "left",
  "githubPullRequests.defaultMergeMethod": "rebase",
  "go.coverOnSave": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "prettier.semi": false,
  "workbench.colorTheme": "Dracula",
  "workbench.iconTheme": "file-icons",
  "gitlens.advanced.telemetry.enabled": false,
  "gitlens.blame.highlight.locations": ["gutter", "line"],
  "terminal.integrated.fontFamily": "Fira Code",
  "gitlens.codeLens.scopes": ["containers"],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "tailwindCSS.emmetCompletions": true,
  "editor.wordWrapColumn": 100,
  "prettier.printWidth": 100,
  "tailwindCSS.includeLanguages": {
    "plaintext": "jsx,tsx"
  },
  "terminal.integrated.experimentalUseTitleEvent": true,
  "tailwindCSS.experimental.showPixelValues": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorBlinking": true
}
